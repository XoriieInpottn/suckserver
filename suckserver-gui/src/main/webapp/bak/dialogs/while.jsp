<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<%@ taglib prefix="s" uri="/struts-tags"%>
<div id="dialog-while" title="Set while">
	<fieldset>
		<label style="display:block;margin-bottom:5px">var</label>
		<input type="text" id="while-var" name="var" style="display:block;width:100%" /> 
	</fieldset>
	<fieldset>
		<label  style="display:block;margin-bottom:5px">start</label>
		<input type="text" id="while-start" placeholder="0" name="start" style="display:block;width:100%"/> 
	</fieldset>
	<fieldset>
		<label style="display:block;margin-bottom:5px">end</label>
		<input type="text" id="while-end" name="end" style="display:block;width:100%" /> 
	</fieldset>
	<fieldset>
		<label style="display:block;margin-bottom:5px">step</label>
		<input type="text" id="while-step" name="step" style="display:block;width:100%" /> 
	</fieldset>
</div>
